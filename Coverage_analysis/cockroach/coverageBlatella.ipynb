   "source": [
    "## Coverage analysis with other reads\n",
    "Input:\n",
    "- Coverage results of other reads\n",
    "    - MaleSoap63.txt\n",
    "    - MaleSoap64.txt\n",
    "    - FemaleSoap65.txt\n",
    "    - FemaleSoap66.txt\n",
    "    - FemaleSoap67.txt\n",
    "    - FemaleSoap68.txt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "e0ed0e97-348d-41ca-9d90-01b48dc834ed",
   "metadata": {},
   "outputs": [],
   "source": [
    "## creates dataframes with coverage results files from other reads\n",
    "## male63: MaleSoap63.txt\n",
    "## male64: MaleSoap64.txt\n",
    "## fem65: FemaleSoap65.txt\n",
    "## fem66: FemaleSoap66.txt\n",
    "## fem67: FemaleSoap67.txt\n",
    "## fem68: FemaleSoap68.txt\n",
    "male63 = pd.read_table('MaleSoap63.txt', delimiter = ' ')\n",
    "male64 = pd.read_table('MaleSoap64.txt', delimiter = ' ')\n",
    "fem65 = pd.read_table('FemaleSoap65.txt', delimiter = ' ')\n",
    "fem66 = pd.read_table('FemaleSoap66.txt', delimiter = ' ')\n",
    "fem67 = pd.read_table('FemaleSoap67.txt', delimiter = ' ')\n",
    "fem68 = pd.read_table('FemaleSoap68.txt', delimiter = ' ')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "270294ac-dfaa-4722-9a03-9914fa80c256",
   "metadata": {},
   "outputs": [],
   "source": [
    "## merges male63, male64, female65, female66, female67 and female68 according to 'Scaffold' in cov2 dataframe\n",
    "cov2 =  fem65.merge(fem66, on = ['Scaffold']).merge(fem67, on = ['Scaffold']).merge(fem68, on = ['Scaffold']).merge(male63, on = ['Scaffold']).merge(male64, on = ['Scaffold'])\n",
    "## mergecov2: dataframe scaffolds in cov2 which length are equal or greater than 1000bp \n",
    "mergecov2 = cov2.loc[(cov2['Length'] >= 1000)]\n",
    "## replaces NaN with zero in each corresponding column in mergecov2 for each coverage depth \n",
    "mergecov2['Mcov63'] = mergecov2['Mcov63'].fillna(0)\n",
    "mergecov2['Mcov64'] = mergecov2['Mcov64'].fillna(0)\n",
    "mergecov2['Fcov65'] = mergecov2['Fcov65'].fillna(0)\n",
    "mergecov2['Fcov66'] = mergecov2['Fcov66'].fillna(0)\n",
    "mergecov2['Fcov67'] = mergecov2['Fcov67'].fillna(0)\n",
    "mergecov2['Fcov68'] = mergecov2['Fcov68'].fillna(0)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "636203b3-71ff-4de0-abc0-31302d75bda0",
   "metadata": {},
   "outputs": [],
   "source": [
    "## adds a column 'maleTotal' in mergecov2 summing up Mcov63, Mco64 columns \n",
    "mergecov2['maleTotal'] = mergecov2['Mcov63']+mergecov2['Mcov64']\n",
    "## adds a column 'femTotal' in mergecov2 summing up Fcov65, Fcov66, Fcov67 and Fcov68 columns \n",
    "mergecov2['femTotal'] = mergecov2['Fcov65']+mergecov2['Fcov66']+mergecov2['Fcov67']+mergecov2['Fcov68']\n",
    "## adds a column 'm+1' in mergecov2 adding one to 'maleTotal' \n",
    "mergecov2['m+1'] = mergecov2['maleTotal']+1\n",
    "## adds a column 'f+1' in mergecov2 adding one to 'femTotal' \n",
    "mergecov2['f+1'] = mergecov2['femTotal']+1\n",
    "## adds a column 'ratio' in mergecov2 and calculates the log2 of 'm+1' over 'f+1'\n",
    "mergecov2['ratio'] = np.log2(mergecov2['m+1']/mergecov2['f+1'])\n",
    "## adds a column 'chromosome' in mergecov2 to assign X/A depending on condition: \n",
    "## if the 'ratio' is less than the median of 'ratio' minus 0.5,\n",
    "## assign X, otherwise A\n",
    "mergecov2['chromosome'] = np.where((mergecov2['ratio'] <(mergecov2.ratio.median()-0.5)),'X','A')"
   ]
